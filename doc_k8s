
# minikube start with nginx volume mount 
minikube start --mount --mount-string="/home/osman/projects/turkai/k8s/nginx:/turkai/nginx"

# kubectl create secret for get docker images
kubectl create secret generic regcred \
	--from-file=.dockerconfigjson=/home/osman/.docker/config.json \
	--type=kubernetes.io/dockerconfigjson

# apply ymls
kubectl apply -f k8s/go-env.yml
kubectl apply -f k8s/nginx-env.yml